{
  "name": "replit",
  "displayName": "replit",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.52.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onFileSystem:replit",
    "onCommand:replit.init"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "replit.init",
        "title": "init",
        "category": "Replit"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "replit.init",
          "when": "workbenchState != workspace"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "build": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts",
    "test": "echo \"no teste :(\""
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.19.12",
    "@types/node-fetch": "^2.5.7",
    "@types/vscode": "^1.52.0",
    "@types/websocket": "^1.0.1",
    "@types/ws": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "eslint": "^7.15.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "typescript": "^4.1.2",
    "vscode-test": "^1.4.1"
  },
  "dependencies": {
    "@replit/crosis": "^6.0.4",
    "@replit/protocol": "^0.2.16",
    "node-fetch": "^2.6.1",
    "ws": "^7.4.2"
  }
}
